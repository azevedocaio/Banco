<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Avalia</servlet-name>
        <servlet-class>Servlet.Avalia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>senhaOK</servlet-name>
        <servlet-class>Servlet.senhaOK</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SenhaIncorreta</servlet-name>
        <servlet-class>Servlet.SenhaIncorreta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ErroJava</servlet-name>
        <servlet-class>Servlet.ErroJava</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mata</servlet-name>
        <servlet-class>Servlet.mata</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Avalia</servlet-name>
        <url-pattern>/Avalia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>senhaOK</servlet-name>
        <url-pattern>/senhaOK</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SenhaIncorreta</servlet-name>
        <url-pattern>/SenhaIncorreta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ErroJava</servlet-name>
        <url-pattern>/ErroJava</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mata</servlet-name>
        <url-pattern>/mata</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
